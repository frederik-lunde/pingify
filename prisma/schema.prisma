generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "mysql"
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

model users {
  id         Int      @id @default(autoincrement())
  name       String
  password   String
  created_at DateTime @default(now())
}
